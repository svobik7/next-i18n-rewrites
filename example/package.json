{
  "name": "next-roots-example",
  "version": "1.0.0",
  "description": "Example of using next-roots package",
  "author": "Jirka Svoboda",
  "license": "MIT",
  "private": true,
  "scripts": {
    "next-roots": "node ../dist/bin/builder.js",
    "dev": "yarn next-roots && next dev",
    "build": "yarn next-roots && next build",
    "start": "next start",
    "analyze": "yarn next-roots && cross-env ANALYZE=true next build",
    "analyze:server": "yarn next-roots && cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "yarn next-roots && cross-env BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "next": "^10.2.3",
    "next-roots": "link:../",
    "react": "link:../node_modules/react",
    "react-dom": "link:../node_modules/react-dom"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.2.3",
    "@types/react": "link:../node_modules/@types/react",
    "@types/react-dom": "link:../node_modules/@types/react-dom",
    "cross-env": "^7.0.2",
    "typescript": "^4.3.2"
  }
}
